{
  "name": "whatsweb-backend",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },

  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },

  "remoteUser": "node",

  "postCreateCommand": "npm install",

  "forwardPorts": [8080],

  "portsAttributes": {
    "8080": {
      "label": "Backend",
      "onAutoForward": "notify"
    }
  }
}
